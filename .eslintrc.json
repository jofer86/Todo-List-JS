{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "rules": {
        "no-shadow": 0,
        "no-param-reassign": 0,
        "eol-last": 0,
        "no-return-assign": 0,
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off",
        "max-len": "off",
        "no-use-before-define": "off",
        "eslint-disable no-alert": "off",
        "no-unused-vars":"off",
        "no-alert":"off",
        "arrow-body-style":"off",
        "arrow-parens":"off"
    }
}